 author = {Nolting, Wolfgang},
 year = {2018},
 title = {Grundkurs Theoretische Physik 1},
 keywords = {Determinante;Differentialrechnung;Dynamik;Erhaltungssätze;Felder;Integralrechnung;Kinematik;Koordinatensysteme;Kreiseltheorie;Matrizen;Planetenbewegungen;Rotation um die eigene Achse;starrer Körper;Trägheitstensor;Vektor;vektorwertgige Funktionen},
